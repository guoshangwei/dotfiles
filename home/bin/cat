#!/usr/bin/env python

import sys
if sys.stdout.isatty() and len(sys.argv) == 2 and not sys.argv[1].startswith("-"):
    try:
        import pygments.cmdline
        pygments.cmdline.main_inner([("-g", "")], sys.argv[1:], "")
        sys.exit(0)
    except ImportError:
        pass

import os
os.execv("/bin/cat", sys.argv)
