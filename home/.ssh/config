# vim: set filetype=sshconfig nospell:

HashKnownHosts yes
Protocol 2

Host faith cs*
    CanonicalizeHostname yes
    CanonicalDomains comp.hkbu.edu.hk
Host bitbucket bitbucket.com
    User git
    HostName bitbucket.com
Host github github.com
    User git
    HostName github.com
Host *.comp.hkbu.edu.hk
    User chengxu
    ForwardAgent yes
Host cs*.comp.hkbu.edu.hk
    ProxyCommand ssh-proxy-helper -H %h -P %p --proxy faith --internal-ip 158.182.0.0/16
    PermitLocalCommand yes
    LocalCommand forward-gpg-agent "%r@%h:%p"
Host haibo
    User xucheng
    HostName haibo-server.xuc.me
    ForwardAgent yes
Host brew
    User brewadmin
    HostName bot.brew.sh
Host lab
    User chengxu@COMP-DEPT
    HostName lab.xuc.me
Host vagrant
    User vagrant
    HostName localhost
    Port 2222
    IdentityFile ~/.local/share/vagrant/insecure_private_key
